<script>
    require(['jquery','underscore', 'Magento_Customer/js/customer-data', 'domReady', 'mage/cookies'], function ($, _, customerData, domReady) {

        $('#pre-login-loader').hide();
        // Clear cookie referenceUrl
        var isCustomerLoginPage = window.location.href.indexOf('customer/account/login'),
            isCustomerCreatePage = window.location.href.indexOf('customer/account/create')
            referenceCookie = $.cookie('referrerUrl'),
            ssoCookieName = 'FSSESSION';

        if ((isCustomerLoginPage === -1) && (isCustomerCreatePage === -1) && referenceCookie != '') {
            $.removeCookie('referrerUrl', {"domain":"www.usa.canon.com","path":"\/shop","secure":true});
        }
        // Reload page without fastly cache to auto login
        $(document).trigger('customer-data-invalidate', ['customer']);
        var customerIsLogin = 0,
            fsCookie = $.cookie(ssoCookieName),
            logOffCookieValue = 'LOGGEDOFF',
            reloadCustomerSectionCookie = 'reloadCustomerSection',
            currentUrl = window.location.href,
            prefixOfParam = (currentUrl.indexOf('?') == -1)? '?' : '&&',
            currentTime = Date.now();
        if (customerIsLogin === 1) {
            if (fsCookie === logOffCookieValue && currentUrl.indexOf('autoLogout') == -1) {
                var targetUrl = currentUrl + prefixOfParam + 'autoLogout=' + currentTime;
                $('#login-loader').show();
                window.location.href = targetUrl;
            }
        } else {
            if (fsCookie != null
                && fsCookie != logOffCookieValue
                && currentUrl.indexOf('autologin') == -1
            ){
                // For redirect to url by preference language after login success
                var targetUrl = currentUrl + prefixOfParam + 'autologin=' + currentTime;
                $.cookie('auto_login_url', currentUrl, {"domain":"www.usa.canon.com","path":"\/shop","secure":true});
                $('#login-loader').show();
                window.location.href = targetUrl;
            }
        }

        /**
         * Events listener
         */
        domReady(function (event, xhr, settings) {
            var needCallReload = $.cookie(reloadCustomerSectionCookie);
            if (customerIsLogin && needCallReload) {
                /*var redirects = ['redirect', 'backUrl'];

                if (_.isObject(xhr.responseJSON) && !_.isEmpty(_.pick(xhr.responseJSON, redirects))) {
                    return;
                } */

                customerData.reload(['customer', 'cart', 'wishlist'], false).done(function (){
                    $(document).trigger('forceReloadCustomerData');
                });

                $.removeCookie(reloadCustomerSectionCookie, {"domain":"www.usa.canon.com","path":"\/shop","secure":true});
                $.removeCookie('auto_login_url', {"domain":"www.usa.canon.com","path":"\/shop","secure":true});
            }
        });
    });
</script>


