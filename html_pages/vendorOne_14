    require(['jquery', 'Magento_Customer/js/customer-data', 'domReady!', 'jquery/ui'], function($, customer_data){
        $(document).ready( function() {
            /* Hide for cart page */
            let cartPagePath = '/checkout/cart';
            if(location.pathname.indexOf(cartPagePath) >= 0){

                var isPaypalHidden = $('.checkout-methods-items #paypalCheckoutExpress').length == 0;


                let cartInterval = setInterval(function() {
                    hideAmazonButton(isPaypalHidden, true);
                    console.log('running');
                    if($(".cart-summary .checkout-methods-items").length > 0) {
                        clearInterval(cartInterval);
                    }
                }, 350);
                
                $(document).ajaxComplete(function (event, xhr, settings) {
                    var isPaypalHidden = $('.checkout-methods-items #paypalCheckoutExpress').length == 0;
                    hideAmazonButton(isPaypalHidden, true);
                });
            }

            /* Hide for checkout page */
            let checkoutPath = '/checkout';
            if(location.pathname.indexOf(checkoutPath) >= 0 && location.pathname.indexOf(cartPagePath) < 0){
                let isDisableAmazon = false;
                $(document).ajaxComplete(function (event, xhr, settings) {
                    let shippingPath = 'shipping-information';
                    if (settings.url.indexOf(shippingPath) >= 0) {
                        hideAmazonButton();
                    }
                });
                $(document).on("ajaxSend", function (event, xhr, settings) {
                    let setPaymentPath = 'cybersource/microform/tokenRequest';
                    if (settings.url.indexOf(setPaymentPath) >= 0) {
                        hideAmazonButton();
                    }
                });

            }

            var hideAmazonButton = function(isPaypalHidden = false, isShoppingCart = false) {
                try {
                    let quote = require('Magento_Checkout/js/model/quote'),
                   cartItems = quote.getItems(),
                   arrDisableAmazon = Array('Backorder', 'PreOrder'),
                   isHandle = false;

                   $.each(cartItems, function(index, item) {
                       if ((!isShoppingCart && $.inArray(item.stock_status, arrDisableAmazon) >= 0) || (isShoppingCart && isPaypalHidden)) {
                            //hide on checkout page.
                            $('[value="amazon_payment_v2"]').parent().parent().hide();
                            $('[value="paypal_express"]').parent().parent().hide();

                            //hide on shopping cart
                            if($(".cart-summary .checkout-methods-items .item > div").length < 2) {
                                $(".amazon-minicart-container").parent().hide();
                            } else {
                                $(".amazon-minicart-container").hide();
                            }

                            isHandle = true;

                       } else if (!isHandle){
                            //hide on checkout page.
                            $('[value="amazon_payment_v2"]').parent().parent().show();
                            $('[value="paypal_express"]').parent().parent().show();

                            //hide on shopping cart
                            if($(".cart-summary .checkout-methods-items .item > div").length < 2) {
                                $(".amazon-minicart-container").parent().show();
                            } else {
                                $(".amazon-minicart-container").show();
                            }

                       }
                   });

                } catch(e) {
                    console.log(e);
                }
            
            }


            /* Hide for mini cart */
            $(document).ajaxComplete(function (event, xhr, settings) {
                let miniCartPaypalButton = $("#minicart-content-wrapper div#paypalCheckoutExpress");
                let miniCartAmazonButton = $("#minicart-content-wrapper div#minicart-amazon-pay-button");
                if(miniCartPaypalButton.length == 0 && miniCartAmazonButton.length > 0) {
                    miniCartAmazonButton.hide();
                }
            });

            $(document).on("mouseenter", ".minicart-event-bound", function (element) {
                let miniCartPaypalButton = $("#minicart-content-wrapper div#paypalCheckoutExpress");
                let miniCartAmazonButton = $("#minicart-content-wrapper div#minicart-amazon-pay-button");
                if(miniCartPaypalButton.length == 0 && miniCartAmazonButton.length > 0) {
                    miniCartAmazonButton.hide();
                }
            });
        });
    });
</script>
