        require([
            'jquery'
        ], function ($)
        {
            $(document).ready(function(){
                function downloadJSAtOnload() {
                    var headerComponent = $('.cmp-header'),
                        mobileSearchBtn = $('.block-search .search-mobile'),
                        mobileSearchMainContainer = headerComponent.find('.account-container-mobile .search-btn.search-mobile');

                    headerComponent.find('.search-button-form').parent().addClass('search-button-form-wrapper');
                    mobileSearchBtn.click(function () {
                        if (headerComponent.find('.search-button-form').is(":visible")) {
                            headerComponent.find('.search-button-form').val('');
                            headerComponent.find('.search-button-form').hide();
                            mobileSearchMainContainer.css({'height': '60px'});
                            $('.search-bar-dropdown-wrapper.mobile').css({'display': 'none'});
                        } else {
                            mobileSearchMainContainer.css({'height': '108px'});
                            headerComponent.find('.search-button-form').show();
                        }
                    });
                }
                if (window.addEventListener)
                    window.addEventListener("load", downloadJSAtOnload, false);
                else if (window.attachEvent)
                    window.attachEvent("onload", downloadJSAtOnload);
                else window.onload = downloadJSAtOnload;
            });
        });
    </script>
