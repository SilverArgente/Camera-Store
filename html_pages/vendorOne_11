<script>
     require(['jquery','Magento_Ui/js/modal/modal'], function($,modal, element) {
        'use strict';
        let options = {
            type: 'popup',
            clickableOverlay: false,
            responsive: true,
            title: $.mage.__('Auto Replenishment Service'),
            modalClass: 'ars-service-modal',
            buttons: [{
                text: $.mage.__('LEARN ABOUT AUTO REPLENISHMENT SERVICE'),
                class: '',
                /**
                 * Click Method
                 */
                click: function () {
                    window.open('https://www.usa.canon.com/ink-paper-toner/printer-ink-subscription-plans/ink-and-toner-cartridge-automatic-replenishment-service', '_blank');
                }
            }],
            closed: function() {
                $('.modals-overlay').remove();
            }
        };

    $('.product-info-main.pdp-info .ars-options .ars-first').click(function () {
        let popUpDiv = $('.ars-modal-description-content').html();
        let targetEl = $('<div>').html($(popUpDiv).html());
        let popup = modal(options, $(targetEl));
        popup.openModal();
    });

    $(document).on('click','#product-service .product-service-text.ars-auto-modal-cart', (function () {
        let popUpDiv = $('.ars-modal-description-content').html();
        let targetEl = $('<div>').html($(popUpDiv).html());
        let popup = modal(options, $(targetEl));
        $('.ars-service-modal').css('z-index',10003);
        popup.openModal();

        $("aside.modal-popup.ars-service-modal.modal-slide._show button.action-close").click(function(){
            $("aside.modal-popup.add-to-cart-popup._show").css("z-index", "10006");
        });
    }));


    let warrantyModalOptions = {
        title: $.mage.__('Canon U.S.A., Inc. Ink & Toner Auto Replenishment Service'),
        type: 'popup',
        modalClass: 'ars-tc-modal',
        responsive: true,
        innerScroll: true,
        autoOpen: false,
        buttons: [],
        self: null,
        closed: function() {
            $('.modals-overlay').remove();
        }
    };

     $(document).on('click','#product-service .ars_terms_check.ars-tc-modal', (function () {
         let popup = modal(warrantyModalOptions, $('#ars-notice'));
         $('.ars-tc-modal').css('z-index',10003);
         popup.openModal();

         $("aside.modal-popup.ars-tc-modal.modal-slide._show button.action-close").click(function(){
             $("aside.modal-popup.add-to-cart-popup._show").css("z-index", "10006");
         });
     }));
});
</script>

