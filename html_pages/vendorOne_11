<div class="page-wrapper"><header class="page-header"><div class="wrap-header-wrapper"><div class="header-component"></div>

    <!-- Load Header Component from the customComponent.js -->
    <script type="text/javascript">
        require(['jquery', 'customComponent', 'domReady!', 'mage/cookies'], function ($) {
            if ($.isEmptyObject(window.aemObj) || !window.aemUrl) {
                return;
            }
            /* MCI-8809 - AEM header pillar category */
            var aemCatgoryJs = "pro,consumer";
            var aemCatgory = aemCatgoryJs.split(",");
            var businessUrl = "https://www.usa.canon.com/shop/catalog/product/index/id/45/?cat=167&color=Black&type=New";
            var proUrl = "https://www.usa.canon.com/shop/catalog/product/index/id/45/?cat=104&color=Black&type=New";
            var consumerUrl = "https://www.usa.canon.com/shop/catalog/product/index/id/45/?cat=7&color=Black&type=New";
            var flag = true;
            var isPdp = "1";
            var headerUrl = aemUrl.header_url;
            var url = headerUrl.replace('.html', '');
            url = url.split(".");
            var lastItem = url.pop();
            var headerMode = localStorage.getItem('header-mode');

            if (aemCatgoryJs != "" && aemCatgory.length > 0) {
                if (aemCatgory.length == 1) {
                    headerMode = aemCatgory[0];
                    flag = true;
                } else if (localStorage.getItem('header-mode-search') &&
                    aemCatgory.includes(localStorage.getItem('header-mode-search'))) {
                    headerMode = localStorage.getItem('header-mode-search');
                    flag = true;
                } else if (aemCatgory.includes(localStorage.getItem('header-mode'))) {
                    headerMode = localStorage.getItem('header-mode');
                    flag = true;
                } else {
                    headerMode = aemCatgory[0];
                    flag = true;
                }
                localStorage.setItem('header-mode', headerMode);
            } else if (isPdp) {
                if (localStorage.getItem('header-mode-search')) {
                    headerMode = localStorage.getItem('header-mode-search');
                    flag = true;
                }
            }

            /*if (window.location.href === consumerUrl) {
                headerMode = 'consumer';
                flag = true;
                localStorage.setItem('header-mode-search', headerMode);
            } else if (window.location.href === proUrl) {
                headerMode = 'pro';
                flag = true;
                localStorage.setItem('header-mode-search', headerMode);
            } else if (window.location.href === businessUrl) {
                headerMode = 'business';
                flag = true;
                localStorage.setItem('header-mode-search', headerMode);
            }*/

            if (flag) {
                if (lastItem == 'plain') {
                    headerUrl = headerUrl.replace(lastItem, 'plain.' + headerMode);
                } else {
                    headerUrl = headerUrl.replace(lastItem, headerMode);
                }
                //localStorage.setItem('header-mode', headerMode);
            }

            if ($.mage.cookies.get('headerModeVal')) {
                $.mage.cookies.clear('headerModeVal');
            }
            $.mage.cookies.set('headerModeVal', headerMode, {path: '/', secure: true});

            aemObj.loadAemContent($('.header-component'), headerUrl);
        });
    </script>
    <script type="text/x-magento-init">
        {
