<script>
    require(['jquery', 'mage/gallery/gallery'], function ($, gallery) {
        var duplicate = 0;
        $(document).on('click', '.fotorama__nav__frame', function () {
            if ($('.fotorama__active').prev().attr('class') == 'fotorama__thumb-border') {
                let productName = $(".page-title span").text();
                let eventName = "e_productInteraction";

                if (window.dataLayer) {
                    duplicate = 1;
                    window.dataLayer.push({
                        'event': eventName,
                        'productName': productName,
                        'interaction': 'galleryimages'
                    });
                }
            } else if ($('.fotorama__fullscreen-icon').is(":visible")) {
                let productName = $(".page-title span").text();
                let eventName = "e_productInteraction";

                if (window.dataLayer) {
                    window.dataLayer.push({
                        'event': eventName,
                        'productName': productName,
                        'interaction': 'galleryimages'
                    });
                }
            }
        });

        $('[data-gallery-role=gallery-placeholder]').on('gallery:loaded', function () {
            var item = $(document).find('.fotorama-item');

            item.on('fotorama:fullscreenenter', function () {
                if (($('.fotorama__active').prev().attr('class') == 'fotorama__thumb-border' && duplicate == 0) ||
                    ($(".fotorama__arr").is(":visible") == false && duplicate == 0)) {
                    let productName = $(".page-title span").text();
                    let eventName = "e_productInteraction";
                    if (window.dataLayer) {
                        window.dataLayer.push({
                            'event': eventName,
                            'productName': productName,
                            'interaction': 'galleryimages'
                        });
                    }
                } else {
                    duplicate = 0;
                }
            });
        });
    });
</script>
