    <script type="text/javascript">
    require(['jquery', 'Magento_Customer/js/customer-data', 'mage/cookies'], function ($, customerData) {
        const cookieNames = 'buy_now,buy_now_aem';
        const rollbackCartParams = 'buy_now_rollback';
        if (cookieNames.split(',').some(cookieName => $.mage.cookies.get(cookieName))) {
            customerData.reload(['cart'], rollbackCartParams);
        }
    });
</script>
