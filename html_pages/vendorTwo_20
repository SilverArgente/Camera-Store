<script type="text/javascript">
	jQuery(document).on('cartJSONFetched cartUpdated', function(evnt) {
		var sendSubmitToLtk = false;
		var sendLtkRequest = false;
		var pageLoaded = '/pg/jsp/product/productclothing.jsp';
		if(evnt.type === 'cartJSONFetched') {
			if (evnt.firingEventType == 'productAddedToCart' || evnt.firingEventType == 'productRemovedFromCart' || pageLoaded.includes('cartthanks')) {
				sendLtkRequest = true;
				var cartJSON = evnt.cartJSON || {},
					cartProducts = cartJSON.products || {},
					productIDs = Object.keys(cartProducts);
				if(productIDs.length) sendSubmitToLtk = true;
				for (var i = productIDs.length - 1; i >= 0; i--) {
					var productID = productIDs[i] + '',
						cartProd = cartProducts[productID],
						unitPrice = cartProd.unitPrice / 100;
						
					_ltk.SCA.AddItemWithLinks(productID, cartProd.quantity, unitPrice, cartProd.name, cartProd.firstImage, cartProd.canonicalURL);
				}
			}
		} else if(evnt.type === 'cartUpdated') {
			sendLtkRequest = true;
			var cartData = evnt.cartData || {},
				cartProductArray = cartData.productArray || [];
			if(cartProductArray.length) sendSubmitToLtk = true;
			for(var track_arr = cartProductArray.length - 1; track_arr >= 0; track_arr--) {
				var cartArrProd = cartProductArray[track_arr],
					cartInfo = cartArrProd.cartInfo,
					product = cartArrProd.product,
					unitPrice = cartInfo.unitPrice / 100;
				_ltk.SCA.AddItemWithLinks(product.productID, cartInfo.quantity, unitPrice, product.name, product.firstImage, product.canonicalURL);	
			}
		}
		if(sendLtkRequest) {
			if( sendSubmitToLtk ) {
		 		_ltk.SCA.Submit();
			} else {
				_ltk.SCA.ClearCart();
			}
		}
	});
</script> 



